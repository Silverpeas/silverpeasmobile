<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:org.silverpeas.mobile.client.components">
  <c:HTMLPanelClickable tag="li" ui:field="container">
      <g:HTML styleName="event-info" ui:field="infos">
        <div class="event-name" ui:field="eventName"></div>
        <div class="event-calendar" ui:field="calendarName"></div>
      </g:HTML>
      <div class="date-event" ui:field="eventDate"></div>
  </c:HTMLPanelClickable>



</ui:UiBinder> 